{
    "name": "jenky/scout-elasticsearch",
    "type": "library",
    "description": "Elasticsearch driver for Laravel Scout",
    "keywords": [
        "jenky",
        "laravel",
        "laravel scout",
        "scout",
        "elasticsearch"
    ],
    "homepage": "https://github.com/jenky/scout-elasticsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "laravel/scout": "^5.0|^6.0",
        "cviebrock/laravel-elasticsearch": "^3.2",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\ScoutElasticsearch\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\ScoutElasticsearch\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
